# ==== SupaFLY 5" Freestyle Preset (BF 4.5.x) ====
set gyro_lowpass2_hz = 250
set gyro_lowpass2_type = PT1
set dterm_lowpass2_hz = 120
set dterm_lowpass2_type = PT1
set dterm_lowpass_hz = 90
set dterm_lowpass_type = PT1
set dyn_notch_range = HIGH
set dyn_notch_count = 2
set dyn_notch_q = 120
set dyn_notch_min_hz = 90
set dyn_notch_max_hz = 500

# ==== PID tune ====
set p_pitch = 65
set i_pitch = 65
set d_pitch = 38
set f_pitch = 145
set p_roll = 60
set i_roll = 60
set d_roll = 35
set f_roll = 140
set p_yaw = 55
set i_yaw = 60
set f_yaw = 110

# ==== Feedforward ====
set feedforward_transition = 0.30
set feedforward_boost = 20

# ==== Rates (SupaFLY feel) ====
set roll_rc_rate = 1.0
set pitch_rc_rate = 1.0
set yaw_rc_rate = 1.0
set roll_srate = 0.80
set pitch_srate = 0.80
set yaw_srate = 0.80
set roll_expo = 0.20
set pitch_expo = 0.20
set yaw_expo = 0.10

# ==== Misc ====
set antigravity_gain = 5
set antigravity_cutoff_hz = 15
set iterm_relax = RP
set iterm_relax_type = GYRO
set iterm_relax_cutoff = 15

save
